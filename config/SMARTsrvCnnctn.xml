<options>
 <!--Use port number > 1024 to avoid privileges for well-known ports. -->
 <networkConnection portNumber="2000" serviceHostName="localhost" retryWaitTimeInS="60"/>
 <!-- S.M.A.R.T. IDs to read (that are interesting for ALL drives)
   Observe ID 190 especially if 194 (Temperature Celsius) is not supported. 
   For ID meanings see file "SMARTattrDefs.xml" in sub-folder "en" -->
 <SMART_parameters_to_observe>1,3,4,5,7,8,9,10,11,12,100,177,179,
  181,182,183,187,190,192,194,195,196,197,198,199,200,201,204,
  232,233,235,240,241,242,250,254</SMART_parameters_to_observe>
</options>
