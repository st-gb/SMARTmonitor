<!--NVMe S.M.A.R.T. attribute definitions in English language -->

<!--see 
https://en.wikipedia.org/wiki/Self-Monitoring,_Analysis_and_Reporting_Technology#Known_NVMe_S.M.A.R.T._attributes
-->
<SMART_attributes> 
  <SMART_attribute ID="1" name="Critical Warning"/>
  <SMART_attribute ID="2" name="Composite Temperature[K]"/>
  <SMART_attribute ID="3" name="Available Spare"/>
  <SMART_attribute ID="4" name="Available Spare Threshold"/>
  <SMART_attribute ID="5" name="Percentage Used"/>
  <SMART_attribute ID="6" name="Data Units Read"/>
  <SMART_attribute ID="7" name="Data Units Written"/>
  <SMART_attribute ID="8" name="Host Read Commands"/>
  <SMART_attribute ID="9" name="Host Write Commands"/>
  <SMART_attribute ID="10" name="Controller Busy Time"/>
  <SMART_attribute ID="11" name="Power Cycles"/>
  <SMART_attribute ID="12" name="Power On Hours"/>
  <SMART_attribute ID="13" name="Unsafe Shutdowns"/>
  <SMART_attribute ID="14" name="Media Errors"/>
  <SMART_attribute ID="15" name="Number of Error Information Log Entries"/>
</SMART_attributes>
